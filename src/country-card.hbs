{{#each this}}
<div>
    <h1>{{name}}</h1>
    <img src="{{flag}}" alt="{{name}}" width="200" height="200">

    <p>Capital: {{capital}}</p>
    <p>Population: {{population}}</p>
    {{#each languages}}
    <ul>Languages:
        <li>
            {{languages.value.name}}
        </li>
    </ul>
    {{/each}}
</div>
{{/each}}